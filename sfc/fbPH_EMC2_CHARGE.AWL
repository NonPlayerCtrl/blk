    //------------------------------------------------------------------------//
    //Log CHARGE end time.
L "dbCONST".CHRONO.CPU_TIME;
T #_time_charge_end;

SET;
S #_log_time_charge_end;
R #sqSeqTimerStart;

    //------------------------------------------------------------------------//
    // VERIFY  VERIFY VERIFY VERIFY VERIFY VERIFY VERIFY VERIFY VERIFY VERIFY //
