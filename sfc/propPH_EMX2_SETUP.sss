Action:- SET;
S #_log_msg_confirm_connect;
R #_prompt_connect_confirm_yes;
Verification:- A "uncondJump";


--------------------------------------------------
TRANSITION::03
--------------------------------------------------
TRNNo:- 03
Title:- Connection correctly inserted
EmptyTransition:- False
Description:- Jump if correct switch conection has been made.
Condition:- //Check switch status for respective connection.

A(;
	A(;
	//If M1 selected, then check the resp. flowpath status.
		A(;
		L #_vessel_mx;
		L "dbCONST".SEL.MX.M1;
		==I ;  
	);
		A(;
		L #_zsc_smfl1_state;
		L "dbCONST".BLK.ZSC2.STATE.IN;
		==I ;  
		);
	);
	//If M2 selected, then check the resp. flowpath status.
	O(;
		A(;
		L #_vessel_mx;
		L "dbCONST".SEL.MX.M2;
		==I ;  
		);
		A(;
		L #_zsc_smfl2_state;
		L "dbCONST".BLK.ZSC2.STATE.IN;
		==I ;  
		);
	);
);

A(;
	A(;
	//If F1P1 selected, then check the resp. flowpath status.
		A(;
		L #_fill_flz;
		L "dbCONST".SEL.FLZ.FL1;
		==I ;  
		);
		A(;
		L #_zsc_bfs1_state;
		L "dbCONST".BLK.ZSC2.STATE.IN;
		==I ;  
		);
	);
	O(;
	//If F2P1 selected, then check the resp. flowpath status.
		A(;
		L #_fill_flz;
		L "dbCONST".SEL.FLZ.FL2;
		==I ;  
		);
		A(;
		L #_zsc_bfs2_state;
		L "dbCONST".BLK.ZSC2.STATE.IN; 
		==I ;  
		);
	);
);


