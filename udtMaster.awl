        ERR_FAULT : BOOL;
        MAN_JUMP : BOOL;
        NO_VERIFIC : BOOL;
        OFF_IDLE : BOOL;
        PAUSED : BOOL;
        RUNNING : Bool;
        ABORTED : Bool;
        S_ACTIVE : BOOL;
        SQ_ACK : Bool;
        SQ_ERROR : Bool;
        SQ_FINISHED : BOOL;
