TYPE "udtModeCommandBLK"
VERSION : 1.0
    STRUCT
        cmdRestartCompleted : Bool;
        hasSFC : Bool;
        transTime : Time := T#10s;               // Selected substate for Block level modules
        lastSUBS : Int := 0;
        lastSTATE : Int := 0;
        permitStart: Bool;
        event : STRUCT;
            interlock : Bool;
            cmdRestart : Bool;
            cmdStart : Bool;
            cmdStop : Bool;
            modeAuto : Bool;
            modeManual : Bool;
            modeOOS : Bool;
        END_STRUCT;
        edge : STRUCT;
            interlock : Bool;
            cmdRestart : Bool;
            cmdStart : Bool;
            cmdStop : Bool;
            modeAuto : Bool;
            modeManual : Bool;
            modeOOS : Bool;
        END_STRUCT;
        flags : STRUCT;
            childInterlock : Bool;
            childFailed : Bool;
            childMismatch : Bool;
            childUnavailable : Bool;
            sfcAborted : Bool;
            sfcCompleted : Bool;
            substateCompleted : Bool;
        END_STRUCT;
        state : STRUCT;
            running : Bool;
            stopped : Bool;
        END_STRUCT;
    END_STRUCT;
END_TYPE

TYPE "udtBLKError"
VERSION : 1.0
    STRUCT
        permitStart : Bool;
        childInterlock : Bool;
        childMismatch : Bool;
        childSafeState : Bool;
        childUnavailable : Bool;
    END_STRUCT;
END_TYPE

TYPE "udtModeCommandSFC"
