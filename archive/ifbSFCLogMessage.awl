        ALARM_OUT                   := "dbHMIBatchReport".BATCH_REPORT_PLC_TRIG.REPORT_EVENT_@@COUNTER@@ ,
        ACKNOWLEDGE                 := "dbHMIBatchReport".BATCH_REPORT_HMI_TRIG.REPORT_EVENT_@@COUNTER@@
    );
    NOP 0;

@@TEMPLATE_END@@
END_FUNCTION_BLOCK
