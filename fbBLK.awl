    A(;
    L #c.@@CHILDPARAMETERALIAS@@.origin.mc.STATE;
    L #c.@@CHILDPARAMETERALIAS@@.owner.mc.CMD;
    <>I;
    );
    JCN _cNoMismatch_@@COUNTERATTR|1@@_@@STATE@@;
    S #flags.childMismatch;

