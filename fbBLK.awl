    T #c.@@CHILDPAIRALIAS@@.owner.@@CHILDPAIRPARAMETER@@;

_pairOutput_@@COUNTERATTR@@_@@CHILDPAIRALIAS@@:   NOP 0;

@@ATTR_END|pPairOutput@@
@@ATTR_END|CHILD@@

